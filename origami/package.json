    "@weborigami/types": "0.0.35",
    "graphviz-wasm": "3.0.1",
    "highlight.js": "11.9.0",
    "marked": "9.1.5",
    "marked-gfm-heading-id": "3.1.0",
    "marked-highlight": "2.0.6",
    "marked-smartypants": "1.1.3",
    "sharp": "0.32.6",
    "yaml": "2.3.3"
  },
  "scripts": {
    "build": "ori exports/buildExports.js src > exports/exports.js",
