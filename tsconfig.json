{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "checkJs": true,
    "noEmit": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "target": "ESNext"
  },
  // "include": ["**/*.js"],
  "include": ["core/**/*.js", "types/**/*.ts"],
  "exclude": ["origami/exports/exports.js"]
}
